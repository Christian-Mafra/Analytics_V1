<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.analytics">
    
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Analytics">
        <activity
            android:name=".activitys.PDFActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.DownloadActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.Visualizacao1Activity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.CadastroActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.TestActivity"
            android:exported="true" />
        <activity
            android:name=".activitys.PrincipalActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.FragmentsActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>